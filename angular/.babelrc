{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }],
    "stage-2"
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-webpack",
        "remove-webpack"
      ]
    },
    "dev": {
      "plugins": [
        "dynamic-import-webpack",
        "remove-webpack"
      ]
    },
    "DEBUG_STORE": {
      "plugins": [
        "dynamic-import-webpack",
        "remove-webpack"
      ]
    }
  },
  "plugins": [
    "transform-runtime"
  ]
}